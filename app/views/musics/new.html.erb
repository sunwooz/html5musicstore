<h1>Add Song</h1>
<%= form_for @music, :html => { :multipart => true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :song %>
  <%= f.file_field :song %><br/><br/>
  <%= f.submit %>
<% end %>
